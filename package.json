{
  "name": "sudokupower",
  "version": "0.1.0",
  "description": "Sudoku generator, solver and player",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run test",
    "postversion": "git push && git push --tags",
    "postinstall": "yarn husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andres-linares/sudokupower.git"
  },
  "keywords": [
    "sudoku",
    "puzzle",
    "multiplayer"
  ],
  "author": "andres-linares",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/andres-linares/sudokupower/issues"
  },
  "homepage": "https://github.com/andres-linares/sudokupower#readme",
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/lodash.clonedeep": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
